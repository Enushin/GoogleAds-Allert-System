# Google Ads API 認証情報
GOOGLE_ADS_DEVELOPER_TOKEN="your-developer-token"
GOOGLE_ADS_CLIENT_ID="your-client-id"
GOOGLE_ADS_CLIENT_SECRET="your-client-secret"
GOOGLE_ADS_REFRESH_TOKEN="your-refresh-token"
GOOGLE_ADS_CUSTOMER_ID="123-456-7890"
# オプション: MCCを利用する場合のみ設定
GOOGLE_ADS_LOGIN_CUSTOMER_ID="987-654-3210"
# 任意: APIエンドポイントを切り替える場合
# GOOGLE_ADS_ENDPOINT="https://googleads.googleapis.com"
# デフォルトAsia/Tokyo。別タイムゾーンを利用する場合のみ指定
# GOOGLE_ADS_TIMEZONE="UTC"
# ローカル検証用のダミートランスポートを利用する場合
# GOOGLE_ADS_TRANSPORT="google_ads_alert.transports.demo:build_transport"
# DEMO_DAILY_COST="50000"
# DEMO_MONTH_TO_DATE_COST="1200000"

# Slack通知設定
SLACK_WEBHOOK_URL="https://hooks.slack.com/services/XXX/YYY/ZZZ"
# 任意設定
# SLACK_ACCOUNT_NAME="Marketing Team"
# SLACK_CURRENCY_SYMBOL="¥"
# SLACK_TIMEZONE="Asia/Tokyo"
# SLACK_INCLUDE_MONTHLY_SECTION="true"
# SLACK_INCLUDE_SPEND_RATE="true"

# スケジュール設定（既定は08:00-20:00の3回実行）
# ALERT_TIMEZONE="Asia/Tokyo"
# ALERT_START_HOUR="8"
# ALERT_START_MINUTE="0"
# ALERT_END_HOUR="20"
# ALERT_END_MINUTE="0"
# ALERT_RUN_COUNT="3"

# 予算目標（任意）
# DAILY_BUDGET="50000"
# MONTHLY_BUDGET="1200000"
